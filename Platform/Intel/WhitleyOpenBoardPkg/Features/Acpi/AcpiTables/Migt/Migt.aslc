  {EFI_MIGT_CR_ACPI_ADDRESS_SPACE_ID,            // IO Type ID.
  EFI_MIGT_CR_ACPI_REGISTER_BIT_WIDTH,
  EFI_MIGT_CR_ACPI_REGISTER_BIT_OFFSET,
  EFI_ACPI_RESERVED_BYTE,
  EFI_MIGT_CR_ACPI_SMI_ADDRESS},

